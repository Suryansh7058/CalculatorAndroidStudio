        android:textColor=" #FFFF00"
        android:textSize="24sp"
        android:textStyle="bold"
        app:layout_constraintDimensionRatio="1:1"
        app:layout_constraintTop_toTopOf="@id/HorizontalGuidelineAdv6666"
        app:layout_constraintBottom_toTopOf="@id/HorizontalGuidelineAdv8333"
        app:layout_constraintLeft_toRightOf="@id/VerticalGuidelineAdv80"
        app:layout_constraintRight_toRightOf="parent" />

    <androidx.appcompat.widget.AppCompatButton
        android:id="@+id/add_adv"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:background="@drawable/operators_button_adv"
        android:text="@string/add"
        android:textColor="#FFFF00"
        android:textSize="30sp"
        android:textStyle="bold"
        app:layout_constraintDimensionRatio="1:1"
        app:layout_constraintLeft_toRightOf="@id/VerticalGuidelineAdv80"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toBottomOf="@id/HorizontalGuidelineAdv8333"
        app:layout_constraintBottom_toTopOf="@id/HorizontalGuidelineAdv8568"/>

    <androidx.appcompat.widget.AppCompatButton
        android:id="@+id/divide_adv"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:background="@drawable/operators_button_adv"
        android:text="@string/divide"
        android:textColor="#FFFF00"
        android:textSize="30sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toTopOf="@id/HorizontalGuidelineAdv4999"
        app:layout_constraintDimensionRatio="1:1"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintLeft_toRightOf="@id/VerticalGuidelineAdv80"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toBottomOf="@id/HorizontalGuidelineAdv33"
        app:layout_constraintVertical_bias="0.478" />

    <androidx.appcompat.widget.AppCompatButton
        android:id="@+id/power_adv"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:background="@color/black"
        android:text="@string/power"
        android:textColor="@color/white"
        android:textSize="20sp"
        app:layout_constraintDimensionRatio="1:1"
        app:layout_constraintTop_toBottomOf="@id/HorizontalGuidelineAdv6666"
        app:layout_constraintBottom_toTopOf="@id/HorizontalGuidelineAdv8333"
        app:layout_constraintLeft_toRightOf="@id/VerticalGuidelineAdv60"
        app:layout_constraintRight_toLeftOf="@id/VerticalGuidelineAdv80"/>


    <androidx.appcompat.widget.AppCompatButton
        android:id="@+id/equal_adv"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:background="@drawable/equal_custom"
        android:text="@string/equal"
        android:textColor=" #FFFFFF"
        android:textSize="30sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintDimensionRatio="1:1"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintLeft_toRightOf="@id/VerticalGuidelineAdv80"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toBottomOf="@id/HorizontalGuidelineAdv8568"
        app:layout_constraintVertical_bias="0.272" />

    <androidx.appcompat.widget.AppCompatButton
        android:id="@+id/multiply_adv"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:background="@drawable/operators_button_adv"
        android:text="@string/multiply"
        android:textColor="#FFFF00"
        android:textSize="30sp"
        android:textStyle="bold"
        app:layout_constraintDimensionRatio="1:1"
        app:layout_constraintTop_toBottomOf="@id/HorizontalGuidelineAdv4999"
        app:layout_constraintBottom_toTopOf="@id/HorizontalGuidelineAdv6666"
        app:layout_constraintLeft_toRightOf="@id/VerticalGuidelineAdv80"
        app:layout_constraintRight_toRightOf="parent" />

    <androidx.appcompat.widget.AppCompatButton
        android:id="@+id/factorial_adv"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:background="@color/black"
        android:text="@string/factorial"
        android:textAllCaps="false"
        android:textColor="@color/white"
        app:layout_constraintDimensionRatio="1:1"
        android:textSize="30sp"
        app:layout_constraintTop_toBottomOf="@+id/HorizontalGuidelineAdv8333"
        app:layout_constraintBottom_toTopOf="@id/HorizontalGuidelineAdv8568"
        app:layout_constraintLeft_toRightOf="@id/VerticalGuidelineAdv60"
        app:layout_constraintRight_toLeftOf="@id/VerticalGuidelineAdv80"
        tools:layout_editor_absoluteX="248dp" />

    <androidx.appcompat.widget.AppCompatButton
        android:id="@+id/e_adv"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:background="@color/black"
        android:text="@string/e"
        android:textAllCaps="false"
        android:textColor="@color/white"
        android:textSize="30sp"
        app:layout_constraintDimensionRatio="1:1"
        app:layout_constraintTop_toBottomOf="@id/HorizontalGuidelineAdv4999"
