<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/black"
    tools:context=".MainActivity">


    <androidx.appcompat.widget.AppCompatButton
        android:id="@+id/degreeButton"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:background="@color/black"
        android:text="DEG"
        android:textAllCaps="false"
        android:textColor="@color/white"
        android:textSize="20sp"
        app:layout_constraintBottom_toTopOf="@id/HorizontalGuidelineAdv16"
        app:layout_constraintDimensionRatio="h,1:1"
        app:layout_constraintLeft_toRightOf="@id/VerticalGuidelineAdv80"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <androidx.appcompat.widget.AppCompatButton
        android:id="@+id/closeBracketButton"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:background="@color/black"
        android:text=")"
        android:textAllCaps="false"
        android:textColor="@color/white"
        android:textSize="30sp"
        app:layout_constraintDimensionRatio="1:1"
        app:layout_constraintBottom_toTopOf="@id/HorizontalGuidelineAdv16"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintLeft_toRightOf="@id/VerticalGuidelineAdv60"
        app:layout_constraintRight_toLeftOf="@id/VerticalGuidelineAdv80"/>

    <androidx.appcompat.widget.AppCompatButton
        android:id="@+id/logButton"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:background="@color/black"
        android:text="log"
        android:textAllCaps="false"
        android:textColor="@color/white"
        android:textSize="24sp"
        app:layout_constraintDimensionRatio="1:1"
        app:layout_constraintBottom_toTopOf="@id/HorizontalGuidelineAdv16"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintLeft_toRightOf="@id/VerticalGuidelineAdv20"
        app:layout_constraintRight_toLeftOf="@id/VerticalGuidelineAdv40"/>

    <androidx.appcompat.widget.AppCompatButton
        android:id="@+id/openBracketButton"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:background="@color/black"
        android:text="("
        android:textAllCaps="false"
        android:textColor="@color/white"
        android:textSize="30sp"
        app:layout_constraintDimensionRatio="1:1"
        app:layout_constraintBottom_toTopOf="@id/HorizontalGuidelineAdv16"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintLeft_toRightOf="@id/VerticalGuidelineAdv40"
        app:layout_constraintRight_toLeftOf="@id/VerticalGuidelineAdv60"/>

    <androidx.appcompat.widget.AppCompatButton
        android:id="@+id/minus_adv"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:background="@drawable/operators_button_adv"
        android:text="@string/minus"
